@using BaseLoginSystemUmbraco.Models.Core
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
    var site = new SiteModel(Model.Content);
}
@section js {
    @Scripts.Render("~/bundles/Scripts")
}
@section css {
    @Styles.Render("~/bundles/Style")
}
@if (Request.Path != "/") {
    @RenderBody()
} else {
    Html.RenderPartial("~/Views/Shared/ContentBuilder/Core/Page.cshtml", site.Pages.DefaultPage);
}